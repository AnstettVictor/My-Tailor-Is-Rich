{% extends 'base.html.twig' %}

{% block title %}Output{% endblock %}

{% block body %}
    <h1>Output</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ output.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ output.name }}</td>
            </tr>
            <tr>
                <th>Amount</th>
                <td>{{ output.amount }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ output.status ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ output.createdAt ? output.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ output.updatedAt ? output.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('output_index') }}">back to list</a>

    <a href="{{ path('output_edit', {'id': output.id}) }}">edit</a>

    {{ include('output/_delete_form.html.twig') }}
{% endblock %}
